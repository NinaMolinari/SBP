{
    "manifest_version": 3,
    "name":             "Browser History Extension",
    "version":          "1.9",
    "description":      "Displays and saves browsing history",
    "action": {
      "default_popup":  "popup.html",
      "default_title":  "Browser History Extension"
    },
    "permissions":      ["history", "downloads", "tabs"]
}